{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://asyncapi.com/mqtt/bindings/operation.json",
  "title": "Operation Schema",
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^x-[\\w\\d\\.\\-\\_]+$": {
      "$ref": "https://github.com/asyncapi/asyncapi-node/blob/master/schemas/2.0.0.json#/definitions/specificationExtension"
    }
  },
  "properties": {
    "qos": {
      "type": "integer"
    },
    "retain": {
      "type": "boolean"
    },
    "bindingVersion": {
      "type": "string",
      "default": "latest"
    }
  }
}
